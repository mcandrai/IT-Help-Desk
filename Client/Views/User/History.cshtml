@{ ViewData["Title"] = "History";
    Layout = "_Layout"; }
<!-- Page Heading -->
<div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h1 class="h3 mb-0 text-gray-800">Tickets History</h1>

</div>
<div class="row">
    <div class="col">
        <table id="ticketTable" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>TICKET ID</th>
                    <th>PROBLEM</th>
                    <th>NAME</th>
                    <th>PRIORITY</th>
                    <th>STATUS</th>
                </tr>
            </thead>
        </table>
    </div>
</div>
@section Scripts {
    <script type="text/javascript" src="~/js/history_user.js"></script>
}