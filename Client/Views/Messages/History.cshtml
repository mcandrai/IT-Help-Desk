@{
    ViewData["Title"] = "Message";
    Layout = "_Layout";
}

<div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h1 class="h3 mb-0 text-gray-800" id="ticket-id"></h1>
</div>
<div class="row">
    <div class="card shadow">
        <div class="card-body">
            <div class="d-flex justify-content-between">
                <div id="userfullname"></div>

            </div>
            <div class="d-flex justify-content-start">
                <p id="message" class="p-2">
                </p>
            </div>
            <div id="detail-message"></div>

        </div>
    </div>
</div>
@section Scripts {
    <script type="text/javascript" src="~/js/detail_message.js"></script>
}